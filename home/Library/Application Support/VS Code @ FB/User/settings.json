{
  "[go]": {
    "editor.defaultFormatter": "nuclide.go"
  },
  "[python]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "nuclide.arclint"
  },
  "[starlark]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "nuclide.arclint"
  },
  "[starlark-skycastle]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "nuclide.arclint"
  },
  "[java]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "nuclide.arclint"
  },
  "[kotlin]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "nuclide.arclint"
  },
  "[objective-c]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "nuclide.cpp"
  },
  "[proto]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "nuclide.arclint"
  },
  "[hack]": {
    "editor.defaultFormatter": "nuclide.hack"
  },
  "[cpp]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "nuclide.arclint"
  },
  "[c]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "nuclide.arclint"
  },
  "[cuda-cpp]": {
    "editor.formatOnSave": true,
    "editor.defaultFxormatter": "nuclide.arclint",
    "editor.defaultFormatter": "nuclide.arclint"
  },
  "[tcc-cpp]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "nuclide.arclint"
  },
  "[objective-cpp]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "nuclide.arclint"
  },
  "[rust]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "nuclide.arclint"
  },
  "[blueprint]": {
    "editor.defaultFormatter": "nuclide.arclint"
  },
  "meta.global.history.enabled": true,
  "[swift]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "nuclide.arclint"
  },
  "[ocaml]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "nuclide.arclint"
  },
  "terminal.integrated.defaultProfile.osx": "zsh",
  "extensions.experimental.deferredStartupFinishedActivation": true,
  "terminal.integrated.systemd.shouldUse": true,
  "window.restoreWindows": "none",
  "accessibility.openChatEditedFiles": false,
  "editor.inlineSuggest.allowSuggestOnTriggerCharacters": true,
  "editor.lightbulb.enabled": "on",
  "window.title": "${devmate:status}${separator}${activeEditorShort}${separator}${rootName}${separator}${profileName}",
  "workbench.secondarySideBar.defaultVisibility": "hidden",
  "emmet.showAbbreviationSuggestions": false,
  "emmet.showExpandedAbbreviation": "never",
  "fb.ms-remote-connections.odtypes": [
    {
      "type": "ondemand_www",
      "flavor": "default",
      "folders": []
    },
    {
      "type": "ondemand_fbcode",
      "flavor": "default",
      "folders": []
    },
    {
      "type": "ondemand_fbsource",
      "flavor": "default",
      "folders": []
    },
    {
      "type": "ondemand_www_fbsource_configerator",
      "flavor": "default",
      "folders": []
    },
    {
      "type": "ondemand_fbcode_configerator_vpnless",
      "flavor": "default",
      "folders": []
    }
  ],
  "debug.saveBeforeStart": "nonUntitledEditorsInActiveGroup",
  "editor.tokenColorCustomizations": {},

  // Font Settings
  "editor.fontFamily": "Monaco, Menlo, 'Courier New', monospace",
  "editor.fontSize": 10,
  "editor.tabSize": 2,
  "editor.rulers": [
    80
  ],

  // I would like to include the following, but that turns off anti aliasing for
  // the entire VS Code UI, which looks very bad for all the other UI elements
  // that use a different size or font.
  //
  // "workbench.fontAliasing": "none",
  //
  // Instead the working around is to add the following CSS by hand into the VS
  // Code main css file.  Which can be found on MacOS at:
  //
  // /Applications/VS Code @ FB.app/Contents/Resources/app/out/vs/workbench/workbench.desktop.main.css
  //
  // .monaco-editor :is(.line-numbers, .view-line) {-webkit-font-smoothing:none;}

  // Make it more emacs like
  "emacs-mcx.useMetaPrefixMacCmd": true,
  "emacs-mcx.wordNavigationStyle": "emacs",
  // Emulate emacs buffers instead of tab groups
  "workbench.editor.showTabs": "single",
  "workbench.editor.enablePreview": false,
  "workbench.editor.revealIfOpen": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.closeEmptyGroups": true,
  // "workbench.editor.limit.enabled": true,
  // "workbench.editor.limit.value": 1,
  // "workbench.editor.limit.perEditorGroup": true,
  "editor.minimap.enabled": false,

  // Currently installed extensions:
  //
  // maplefactory.tiller
  // stkb.rewrap
  // tuttieee.emacs-mcx

  // Settings customized through UI
  "rewrap.wholeComment": false,
  "wasabi.interactiveDevelopment.introMessageNumTimesShown": 10,
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.inheritEnv": false,
  "devmate.inline.assertiveness.enable-inline-buttons-highlighted-text": false,
  "terminal.integrated.cwd": "${fileDirname}",
  "devmate.sidebar.autoRunAllowedCommands": [
    "tw job:*",
    "head:*"
  ],
  "workbench.editor.navigationScope": "editorGroup",
}

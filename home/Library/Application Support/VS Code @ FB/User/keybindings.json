// Place your key bindings in this file to override the defaults
[
  {
    "key": "shift+left",
    "command": "workbench.action.focusPreviousGroup"
  },
  {
    "key": "shift+right",
    "command": "workbench.action.focusNextGroup",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+x ctrl+f",
    "command": "tiller.show"
  },
  {
    "key": "ctrl+p",
    "command": "-tiller.show"
  },
  {
    "key": "ctrl+l",
    "command": "tiller.back",
    "when": "inTiller"
  },
  {
    "key": "enter",
    "command": "tiller.forward",
    "when": "inTiller"
  },
  {
    "key": "ctrl+shift+right",
    "command": "workbench.action.moveActiveEditorGroupRight"
  },
  {
    "key": "cmd+q",
    "command": "rewrap.rewrapComment",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+q",
    "command": "-rewrap.rewrapComment",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+c ctrl+c",
    "command": "codelens.showLensesInCurrentLine",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+c m",
    "command": "buck.lsp.test_owning_target",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+c .",
    "command": "testing.runAtCursor",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+cmd+.",
    "command": "emacs-mcx.findDefinitions",
    "when": "config.emacs-mcx.useMetaPrefixMacCmd && editorHasDefinitionProvider && editorTextFocus"
  },
  {
    "key": "cmd+.",
    "command": "-emacs-mcx.findDefinitions",
    "when": "config.emacs-mcx.useMetaPrefixMacCmd && editorHasDefinitionProvider && editorTextFocus"
  },
  {
    "key": "ctrl+c b",
    "command": "buck.lsp.build_owning_target",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+shift+left",
    "command": "workbench.action.moveEditorToLeftGroup"
  },
  {
    "key": "ctrl+c enter",
    "command": "buck.lsp.run_owning_target",
    "when": "editorFocus"
  },
  {
    "key": "alt+cmd+u",
    "command": "code-compose.chat.toggle",
    "when": "code-compose.isLoaded"
  },
  {
    "key": "cmd+u",
    "command": "-code-compose.chat.toggle",
    "when": "code-compose.isLoaded"
  },
  {
    "key": "shift+cmd+-",
    "command": "redo"
  }
]
